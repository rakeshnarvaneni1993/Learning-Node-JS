{{>header}}

<div class="container">
	<div class="wrapper">
	<table class="table ">
	    <thead>
	      <tr>
	        <th>Logo</th>
	        <th>Name</th>
	        <th>Price</th>
	        <th>Total Coins</th>
	      </tr>
	    </thead>
	    <tbody>
			{{#each data}}
	    		 <tr>
				      <td><img src={{this.imageUrl}} alt='coin image' ></td>
				      <td>{{this.name}}</td>
				      <td>{{this.price}}</td>
				      <td>{{this.totalCoins}}</td>
			    </tr>
			{{/each}}
	    </tbody>
	  </table>
	</div>
</div>

{{> endingTags}}



